{
  "name": "oneclickpdf",
  "version": "1.0.0",
  "description": "Professional PDF processing web application",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "keywords": [
    "pdf",
    "converter",
    "editor",
    "merge",
    "split",
    "compress"
  ],
  "author": "OneClickPDF Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "pdf-lib": "^1.17.1"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "form-data": "^4.0.4"
  }
}
